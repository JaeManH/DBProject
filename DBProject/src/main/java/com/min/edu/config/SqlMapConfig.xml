<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>

	<properties resource="properties/db.properties" />

	<typeAliases>
		<typeAlias type="com.min.edu.vo.MemberVo" alias="memberVo"/>
	<typeAlias type="com.min.edu.vo.MemberVo" alias="membervo"/>
	<typeAlias type="com.min.edu.vo.ProductVo" alias="productvo"/>
	<typeAlias type="com.min.edu.vo.CouponVo" alias="couponvo"/>
	<typeAlias type="com.min.edu.vo.BoardVo" alias="boardvo"/>
	<typeAlias type="com.min.edu.vo.QnAVo" alias="qnavo"/>
	<typeAlias type="com.min.edu.vo.QnAVo" alias="qna_c"/>
	<typeAlias type="com.min.edu.vo.AdminVo" alias="adminvo"/>
	</typeAliases>
	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="${driver}" />
				<property name="url" value="${url}" />
				<property name="username" value="${username}" />
				<property name="password" value="${password}" />
			</dataSource>


		</environment>
	</environments>

	<mappers>
	<mapper resource="com/min/edu/mapper/Member.xml"/>
	<mapper resource="com/min/edu/mapper/Member.xml"/>
	<mapper resource="com/min/edu/mapper/Product.xml"/>
	<mapper resource="com/min/edu/mapper/Coupon.xml" />
	<mapper resource="com/min/edu/mapper/Board.xml" />
	<mapper resource="com/min/edu/mapper/QnA.xml" />
	<mapper resource="com/min/edu/mapper/Admin.xml" />
	</mappers>

</configuration>
